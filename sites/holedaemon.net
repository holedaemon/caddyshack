www.holedaemon.net, holedaemon.net {
	import static-site holedaemon.net

	rewrite /posts/* blog.html

	handle /posts/* {
		file_server browse
	}

	handle /images/* {
		file_server browse
	}

	handle /box/* {
		file_server browse
	}

	handle /jb/* {
		file_server browse
	}

	handle /jb_staging/* {
		file_server browse
	}
}

turnttable.holedaemon.net {
	reverse_proxy turnt:9998
}

bot.holedaemon.net {
	reverse_proxy bot2:9997
}